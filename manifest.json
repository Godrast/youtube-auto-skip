{
    "manifest_version": 2,
    "name": "Youtube auto ad skip",
    "description": "This extension will automatically skip ads on youtube",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "main.js"
        ]
    }],
    "permissions": [
        "activeTab",
        "tabs"
    ]
}